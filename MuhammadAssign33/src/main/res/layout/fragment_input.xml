<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FF0000"
    tools:context=".MuhammadActivity">

    <TextView
        android:id="@+id/muhammadtv2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:gravity="top|center"
        android:text="@string/secondfragtv"
        android:textColor="#43a047"
        android:textSize="40sp"
        android:textStyle="italic|bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/muhammadedET"
        android:layout_width="265dp"
        android:layout_height="81dp"
        android:layout_marginStart="73dp"
        android:layout_marginTop="123dp"
        android:ems="10"
        android:hint="@string/secondfraget"
        android:inputType="numberPassword"
        android:maxLength="6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/muhammadimageButton"
        android:layout_width="270dp"
        android:layout_height="249dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="256dp"
        android:src="@drawable/jinx"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>